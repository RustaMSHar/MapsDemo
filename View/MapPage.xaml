<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:desk_maps="clr-namespace:Microsoft.Maui.Controls.Maps;assembly=Microsoft.Maui.Controls.Maps"
             xmlns:maps2="http://schemas.microsoft.com/dotnet/2021/maui/maps"
             xmlns:map="clr-namespace:Microsoft.Maui.Controls.Maps;assembly=Microsoft.Maui.Controls.Maps"
             xmlns:maps="clr-namespace:Microsoft.Maui.Controls.Maps;assembly=Microsoft.Maui.Controls.Maps"
             xmlns:loc="clr-namespace:Microsoft.Maui.Devices.Sensors;assembly=Microsoft.Maui.Essentials"
             xmlns:ctrl="clr-namespace:MapsDemo.Controls"
             x:Class="MapsDemo.MainPage">
    
    <StackLayout>
        <ctrl:MapEx x:Name="MyMap"
                    IsShowingUser="True" 
                    VerticalOptions="FillAndExpand"
                    CustomPins="{Binding Pins}">

        </ctrl:MapEx>
    </StackLayout>



</ContentPage>

